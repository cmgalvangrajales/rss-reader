{
  "name": "rss-reader-ui",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "env": "node config/scripts/index.js",
    "dev": "cross-env ENV=local npm run env && vite",
    "build": "npm run env && tsc && vite build",
    "build:local": "cross-env ENV=local npm run build",
    "build:dev": "cross-env ENV=dev npm run build",
    "build:prod": "cross-env ENV=prod npm run build",
    "build:staging": "cross-env ENV=staging npm run build",
    "format:fix": "eslint --fix --max-warnings=0 --ext=js,jsx,ts,tsx src && prettier -w src",
    "format:check": "eslint --fix --max-warnings=0 --ext=js,jsx,ts,tsx src && prettier --check src",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "antd": "^5.8.5",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/antd": "^1.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
